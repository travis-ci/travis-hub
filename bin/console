#!/usr/bin/env ruby

$: << 'lib'

require 'irb'
require 'travis/hub'

Job.class_eval do
  class << self
    def running
      where(state: [:queued, :received, :started]).order('jobs.id')
    end

    def since(time)
      where('updated_at < ?', time)
    end
  end
end

Travis::Hub::Context.new

IRB.start(__FILE__)
